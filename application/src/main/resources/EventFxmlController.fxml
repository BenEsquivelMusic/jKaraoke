<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="addSongAnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="174.0" prefWidth="441.0"
            stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1"
            fx:controller="karaoke.controller.EventFxmlController">
    <GridPane prefHeight="190.0" prefWidth="441.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="142.0" minWidth="10.0" prefWidth="66.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="268.0" minWidth="10.0" prefWidth="266.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="118.0" minWidth="10.0" prefWidth="85.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <Label layoutX="38.0" layoutY="36.0" text="Event"/>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
            <TextField fx:id="txtEventName" layoutY="31.0" prefHeight="25.0" prefWidth="268.0"/>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
            <Label layoutX="21.0" layoutY="20.0" prefHeight="17.0" prefWidth="47.0" text="Save File"/>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <TextField fx:id="txtEventFile" layoutY="18.0" prefHeight="25.0" prefWidth="268.0"/>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <Button layoutX="14.0" layoutY="18.0" mnemonicParsing="false"
                    onAction="#handleBrowseEventFile" text="Browse"/>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
            <ButtonBar prefHeight="58.0" prefWidth="268.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                       AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <buttons>
                    <Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancelAction"
                            style="-fx-background-color: red;" text="Cancel"/>
                    <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOkAction"
                            style="-fx-background-color: green;" text="Ok"/>
                </buttons>
            </ButtonBar>
        </AnchorPane>
    </GridPane>
</AnchorPane>
