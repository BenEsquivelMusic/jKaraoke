<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.media.MediaView?>
<?import karaoke.media.CdgMediaView?>
<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="mediaViewPane" stylesheets="@style.css"
      fx:controller="karaoke.controller.MediaViewFxmlController"
      xmlns="http://javafx.com/javafx/23.0.1">
    <Canvas fx:id="mediaViewCanvas"/>
    <CdgMediaView fx:id="cdgMediaView" fitHeight="${mediaViewPane.height}" fitWidth="${mediaViewPane.width}"/>
    <MediaView fx:id="mediaView" fitHeight="${mediaViewPane.height}" fitWidth="${mediaViewPane.width}"
               preserveRatio="false"/>
</Pane>
